// ER Diagram
digraph {
	rankdir=TB
	node [shape=plaintext style="rounded,filled"]
	graph [nodesep=1.0 ranksep=1.5 splines=polyline]
	edge [arrowsize=0.8]
	students [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">students</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>student_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">admission_cohort</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">program_major_history</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">academic_status</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	courses [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">courses</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>course_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">prerequisite_courses</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">equivalence_mappings</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	enrollments [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">enrollments</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>enrollment_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">student_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">course_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">attempt_number</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">grade</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">retake_flag</TD><TD ALIGN="LEFT">BOOL</TD></TR><TR><TD ALIGN="LEFT">withdrawal_flag</TD><TD ALIGN="LEFT">BOOL</TD></TR><TR><TD ALIGN="LEFT">incomplete_flag</TD><TD ALIGN="LEFT">BOOL</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	term_aggregates [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">term_aggregates</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>term_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">student_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">gpa</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">credits_attempted</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">credits_completed</TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	enrollments -> students [label=student_id arrowhead=crow color=black style=dashed]
	enrollments -> courses [label=course_id arrowhead=crow color=black style=dashed]
	term_aggregates -> students [label=student_id arrowhead=crow color=black style=dashed]
}
