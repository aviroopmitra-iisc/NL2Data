// ER Diagram
digraph {
	rankdir=TB
	node [shape=plaintext style="rounded,filled"]
	graph [nodesep=1.0 ranksep=1.5 splines=polyline]
	edge [arrowsize=0.8]
	card_transaction [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">card_transaction</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>transaction_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">transaction_datetime</TD><TD ALIGN="LEFT">DATETIME</TD></TR><TR><TD ALIGN="LEFT">customer_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">card_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">merchant_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">merchant_geo_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">card_home_geo_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">amount_usd</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">currency_code</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">exchange_rate</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">amount_local_currency</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">mcc_code</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">is_card_present</TD><TD ALIGN="LEFT">BOOL</TD></TR><TR><TD ALIGN="LEFT">confirmed_fraud</TD><TD ALIGN="LEFT">BOOL</TD></TR><TR><TD ALIGN="LEFT">fraud_pattern_label</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	dim_customer [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">dim_customer</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>customer_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">customer_name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">segment</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">home_geo_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	dim_card [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">dim_card</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>card_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">customer_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">card_network</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">product_type</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	dim_merchant [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">dim_merchant</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>merchant_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">merchant_name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">category</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">geo_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	dim_geography [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">dim_geography</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>geo_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">country_code</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">region</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">city</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	card_transaction -> dim_customer [label=customer_id arrowhead=crow color=black style=dashed]
	card_transaction -> dim_card [label=card_id arrowhead=crow color=black style=dashed]
	card_transaction -> dim_merchant [label=merchant_id arrowhead=crow color=black style=dashed]
	card_transaction -> dim_geography [label=merchant_geo_id arrowhead=crow color=black style=dashed]
	card_transaction -> dim_geography [label=card_home_geo_id arrowhead=crow color=black style=dashed]
	dim_customer -> dim_geography [label=home_geo_id arrowhead=crow color=black style=dashed]
	dim_card -> dim_customer [label=customer_id arrowhead=crow color=black style=dashed]
	dim_merchant -> dim_geography [label=geo_id arrowhead=crow color=black style=dashed]
}
