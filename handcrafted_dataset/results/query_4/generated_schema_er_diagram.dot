// ER Diagram
digraph {
	rankdir=TB
	node [shape=plaintext style="rounded,filled"]
	graph [nodesep=1.0 ranksep=1.5 splines=polyline]
	edge [arrowsize=0.8]
	patient [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">patient</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>patient_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">age</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">gender</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">comorbid_conditions</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	provider [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">provider</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>provider_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">specialty</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	facility [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">facility</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>facility_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">location</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	diagnosis [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">diagnosis</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>diagnosis_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">code</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">description</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	procedure [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">procedure</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>procedure_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">code</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">description</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	medication [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">medication</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>medication_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">dosage</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	encounter [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">encounter</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>encounter_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">patient_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">provider_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">facility_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">date</TD><TD ALIGN="LEFT">DATETIME</TD></TR><TR><TD ALIGN="LEFT">reason</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">policy_change_effective</TD><TD ALIGN="LEFT">BOOL</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	diagnosis_encounter [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#757575" COLSPAN="2"><B><FONT COLOR="white">diagnosis_encounter</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>diagnosis_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT"><B>encounter_id</B></TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#757575" fillcolor="#F8F8F8" style="rounded,filled"]
	procedure_encounter [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#757575" COLSPAN="2"><B><FONT COLOR="white">procedure_encounter</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>procedure_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT"><B>encounter_id</B></TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#757575" fillcolor="#F8F8F8" style="rounded,filled"]
	medication_encounter [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#757575" COLSPAN="2"><B><FONT COLOR="white">medication_encounter</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>medication_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT"><B>encounter_id</B></TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#757575" fillcolor="#F8F8F8" style="rounded,filled"]
	encounter -> patient [label=patient_id arrowhead=crow color=black style=dashed]
	encounter -> provider [label=provider_id arrowhead=crow color=black style=dashed]
	encounter -> facility [label=facility_id arrowhead=crow color=black style=dashed]
	diagnosis_encounter -> diagnosis [label=diagnosis_id arrowhead=crow color=black style=dashed]
	diagnosis_encounter -> encounter [label=encounter_id arrowhead=crow color=black style=dashed]
	procedure_encounter -> procedure [label=procedure_id arrowhead=crow color=black style=dashed]
	procedure_encounter -> encounter [label=encounter_id arrowhead=crow color=black style=dashed]
	medication_encounter -> medication [label=medication_id arrowhead=crow color=black style=dashed]
	medication_encounter -> encounter [label=encounter_id arrowhead=crow color=black style=dashed]
}
