// ER Diagram
digraph {
	rankdir=TB
	node [shape=plaintext style="rounded,filled"]
	graph [nodesep=1.0 ranksep=1.5 splines=polyline]
	edge [arrowsize=0.8]
	sales_fact [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">sales_fact</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>transaction_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">product_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">customer_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">quantity_sold</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">sale_amount</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">transaction_date</TD><TD ALIGN="LEFT">DATETIME</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	product_catalog [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">product_catalog</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>product_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">product_name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">category</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">price</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">launch_date</TD><TD ALIGN="LEFT">DATETIME</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	customer_dimension [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">customer_dimension</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>customer_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">customer_name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">segment</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">email</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">registration_date</TD><TD ALIGN="LEFT">DATETIME</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	events_log [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">events_log</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>event_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">product_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">customer_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">event_type</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">event_timestamp</TD><TD ALIGN="LEFT">DATETIME</TD></TR><TR><TD ALIGN="LEFT">recommended_product_id</TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	sales_fact -> product_catalog [label=product_id arrowhead=crow color=black style=dashed]
	sales_fact -> customer_dimension [label=customer_id arrowhead=crow color=black style=dashed]
	events_log -> product_catalog [label=product_id arrowhead=crow color=black style=dashed]
	events_log -> customer_dimension [label=customer_id arrowhead=crow color=black style=dashed]
}
