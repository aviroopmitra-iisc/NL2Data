// ER Diagram
digraph {
	rankdir=TB
	node [shape=plaintext style="rounded,filled"]
	graph [nodesep=1.0 ranksep=1.5 splines=polyline]
	edge [arrowsize=0.8]
	account [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">account</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>account_id</B></TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	plan [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">plan</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>plan_id</B></TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	subscription [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">subscription</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>subscription_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">account_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">plan_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">status</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	plan_history [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">plan_history</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>subscription_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT"><B>effective_from</B></TD><TD ALIGN="LEFT">DATE</TD></TR><TR><TD ALIGN="LEFT">plan_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">effective_to</TD><TD ALIGN="LEFT">DATE</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	invoice [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">invoice</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>invoice_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">account_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">invoice_date</TD><TD ALIGN="LEFT">DATE</TD></TR><TR><TD ALIGN="LEFT">due_date</TD><TD ALIGN="LEFT">DATE</TD></TR><TR><TD ALIGN="LEFT">total_amount</TD><TD ALIGN="LEFT">FLOAT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	invoice_line_item [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">invoice_line_item</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>invoice_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT"><B>line_number</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">subscription_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">plan_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">add_on_code</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">quantity</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">amount</TD><TD ALIGN="LEFT">FLOAT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	subscription -> account [label=account_id arrowhead=crow color=black style=dashed]
	subscription -> plan [label=plan_id arrowhead=crow color=black style=dashed]
	plan_history -> subscription [label=subscription_id arrowhead=crow color=black style=dashed]
	plan_history -> plan [label=plan_id arrowhead=crow color=black style=dashed]
	invoice -> account [label=account_id arrowhead=crow color=black style=dashed]
	invoice_line_item -> invoice [label=invoice_id arrowhead=crow color=black style=dashed]
	invoice_line_item -> subscription [label=subscription_id arrowhead=crow color=black style=dashed]
	invoice_line_item -> plan [label=plan_id arrowhead=crow color=black style=dashed]
}
