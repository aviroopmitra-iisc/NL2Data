// ER Diagram
digraph {
	rankdir=TB
	node [shape=plaintext style="rounded,filled"]
	graph [nodesep=1.0 ranksep=1.5 splines=polyline]
	edge [arrowsize=0.8]
	trips [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#4A90E2" COLSPAN="2"><B><FONT COLOR="white">trips</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>trip_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">rider_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">driver_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">geographic_zone_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">vehicle_type_id (FK)</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">duration</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">distance</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">surge_multiplier</TD><TD ALIGN="LEFT">FLOAT</TD></TR><TR><TD ALIGN="LEFT">start_time</TD><TD ALIGN="LEFT">DATETIME</TD></TR><TR><TD ALIGN="LEFT">end_time</TD><TD ALIGN="LEFT">DATETIME</TD></TR><TR><TD ALIGN="LEFT">transaction_date</TD><TD ALIGN="LEFT">DATE</TD></TR><TR><TD ALIGN="LEFT">hour_of_day</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">day_of_week</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">day_of_month</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">month</TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">year</TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#4A90E2" fillcolor="#E8F4F8" style="rounded,filled"]
	riders [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">riders</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>rider_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">email</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">phone_number</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	drivers [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">drivers</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>driver_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">license_number</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">status</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	geographic_zones [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">geographic_zones</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>zone_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">zone_name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">city</TD><TD ALIGN="LEFT">TEXT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	vehicle_types [label=<<TABLE BORDER="2" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4"><TR><TD BGCOLOR="#7CB342" COLSPAN="2"><B><FONT COLOR="white">vehicle_types</FONT></B></TD></TR><TR><TD ALIGN="LEFT"><B>vehicle_type_id</B></TD><TD ALIGN="LEFT">INT</TD></TR><TR><TD ALIGN="LEFT">type_name</TD><TD ALIGN="LEFT">TEXT</TD></TR><TR><TD ALIGN="LEFT">capacity</TD><TD ALIGN="LEFT">INT</TD></TR></TABLE>> color="#7CB342" fillcolor="#F0F8E8" style="rounded,filled"]
	trips -> riders [label=rider_id arrowhead=crow color=black style=dashed]
	trips -> drivers [label=driver_id arrowhead=crow color=black style=dashed]
	trips -> geographic_zones [label=geographic_zone_id arrowhead=crow color=black style=dashed]
	trips -> vehicle_types [label=vehicle_type_id arrowhead=crow color=black style=dashed]
}
